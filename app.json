      "predictiveBackGestureEnabled": false,
      "permissions": [
        "NOTIFICATIONS",
        "NOTIFICATIONS"
      ],
      "package": "com.anonymous.DrinkUp"
    },
    "web": {
      "favicon": "./assets/drinkup_logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "650256e6-8d63-44fb-93a2-8ae85deeff11"
      }
